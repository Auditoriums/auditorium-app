{
  "expo": {
    "name": "Auditorium",
    "slug": "auditorium-app",
    "version": "1.0.17",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "auditorium",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ua.com.knmau.auditorium",
      "buildNumber": "1.0.17",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "backgroundColor": "#ffffff",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "NOTIFICATIONS",
        "USER_FACING_NOTIFICATIONS"
      ],
      "package": "com.knmau.auditorium",
      "versionCode": 16
    },
    "web": {
      "startUrl": "https://auditorium.knmau.com.ua/",
      "favicon": "./assets/images/favicon.png",
      "backgroundColor": "#ffffff",
      "display": "standalone"
    }
  }
}
